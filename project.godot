; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="Leather Engine"
run/main_scene="res://Scenes/Title Screen.tscn"
boot_splash/image="res://Assets/Images/Splash Screen.png"
boot_splash/bg_color=Color( 0, 0, 0, 1 )
config/icon="res://Assets/Images/iconOG.png"
config/windows_native_icon="res://Assets/Images/icon.ico"

[audio]

output_latency=0
output_latency.web=0

[autoload]

Paths="*res://Scripts/Autoload/Paths.gd"
Settings="*res://Scripts/Autoload/Settings.gd"
Scenes="*res://Scripts/Autoload/SceneHandler.gd"
AudioHandler="*res://Scenes/Audio Handler.tscn"
GameplaySettings="*res://Scripts/Autoload/Gameplay Settings.gd"
Conductor="*res://Scripts/Autoload/Conductor.gd"
NoteFunctions="*res://Scripts/Autoload/NoteFunctions.gd"
VolumeSelector="*res://Scenes/Volume Selector.tscn"
Fps="*res://Scenes/FPS.tscn"
Keybinds="*res://Scripts/Autoload/Keybinds.gd"
ModLoader="*res://Scripts/Autoload/Mod Loader.gd"

[display]

window/size/width=1280
window/size/height=720
window/vsync/use_vsync=false
window/stretch/mode="2d"
window/stretch/aspect="keep"

